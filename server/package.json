{
  "name": "street-photography-server",
  "version": "1.0.0",
  "description": "Node.js server for Re-Earth visualizer plugin with CMS integration",
  "main": "index.js",
  "scripts": {
    "dev": "vercel dev --listen 5200",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@vercel/node": "^3.0.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "form-data": "^4.0.4",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/form-data": "^2.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^9.37.0",
    "eslint-config-reearth": "^0.3.8",
    "prettier": "^3.6.2",
    "supertest": "^6.3.3",
    "typescript": "^5.0.0",
    "vercel": "^32.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.10.3"
}
